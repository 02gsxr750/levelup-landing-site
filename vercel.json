{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "public": "dist/public",
  "functions": {
    "dist/index.cjs": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.cjs"
    }
  ]
}
